<%include partials/header%>

<h1><%=spaceground.name%></h1>

<img src="<%= spaceground.image%>">
<p><%=spaceground.description%></p>

<% spaceground.comments.forEach(function(comment){ %>
  <p>
    <strong><%= comment.author %> </strong>
      - <%= comment.text %>
  </p>
<% }); %>

<%include partials/footer%>
